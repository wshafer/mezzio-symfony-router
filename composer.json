{
    "name": "wshafer/mezzio-symfony-router",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Westin Shafer",
            "email": "shafer_w2002@yahoo.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0.1",
        "symfony/psr-http-message-bridge": "^2.0",
        "symfony/routing": "^5.0",
        "mezzio/mezzio-router": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "@stable",
        "phpmd/phpmd" : "@stable",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "WShafer\\Mezzio\\Symfony\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WShafer\\Mezzio\\Symfony\\Router\\Test\\": "tests/"
        }
    },
    "extra": {
        "laminas": {
            "config-provider": "WShafer\\Mezzio\\Symfony\\Router\\ConfigProvider"
        }
    }
}
